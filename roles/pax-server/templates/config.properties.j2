#Note that number of columns are not configuratble because it is determined by the project script.

px.project.raw.script.input.json.path=/usr/local/paxata/server/test/2018_2_test.json
px.temp.file.dir=/usr/local/paxata/server/test/

#Values below means the app will run same project script 1 time.
# 1st time with 10-row base dataset and 2-row lookup/append dataset

px.base.file.line.counts=10
px.attach.file.line.counts=2

#list of fixed values according to column type.
# Number of elements should be the same for each type.
# empty value also works
px.fixed.text.values=one,two,three
px.fixed.number.values=999,,777
px.fixed.date.values=2000,2001,
px.fixed.boolean.values=TRUE,TRUE,TRUE

#example: 40% of the dataset rows have random value, while the other 60% have fixed values defined above.
px.fixed.line.percentage=0

#Values below means all datasets random rows will have 10 characters in text columns and 3 digits in number columns.
px.uniform.text.column.width=10
px.uniform.number.column.width=5

px.project.name=AnsibleGenerateEndToEndTest_OkToDelete

px.base.url=https://localhost:{{ jetty_https_port }}
px.auth.username={{ rest_user }}
px.auth.token={{ rest_password }}

